# Donmoa 환경 변수 설정 파일
# 이 파일을 복사하여 .env 파일로 만들고 실제 값들을 입력하세요

# 증권사 API 설정
SECURITIES_API_KEY=your_securities_api_key_here
SECURITIES_SECRET=your_securities_secret_here
SECURITIES_ACCOUNT_NO=your_account_number_here

# 은행 API 설정
BANK_API_KEY=your_bank_api_key_here
BANK_SECRET=your_bank_secret_here
BANK_ACCOUNT_NO=your_bank_account_number_here

# 거래소 API 설정
EXCHANGE_API_KEY=your_exchange_api_key_here
EXCHANGE_SECRET=your_exchange_secret_here
EXCHANGE_PASSPHRASE=your_exchange_passphrase_here

# 기타 설정
DONMOA_ENV=development
DONMOA_DEBUG=false
